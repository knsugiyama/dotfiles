{
  "title": "my rule",
  "rules": [
    {
      "description": "left_command -> left_control, left_control -> left_command in item2 etc.",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "left_command",
            "modifiers": {
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "left_control",
              "lazy": true
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^com.googlecode.iterm2",
                "^com.microsoft.VSCode",
                "^com.jetbrains.intellij"
              ]
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "left_control",
            "modifiers": {
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "left_command",
              "lazy": true
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^com.googlecode.iterm2",
                "^com.microsoft.VSCode",
                "^com.jetbrains.intellij"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "escキーを押したときに、英数キーも送信する",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "escape"
          },
          "to": [
            {
              "key_code": "escape"
            },
            {
              "key_code": "japanese_eisuu"
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^com.googlecode.iterm2",
                "^com.microsoft.VSCode",
                "^com.jetbrains.intellij"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "Ctrl+[を押したときに、英数キーも送信する",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "open_bracket",
            "modifiers": {
              "mandatory": ["control"]
            }
          },
          "to": [
            {
              "key_code": "open_bracket",
              "modifiers": ["control"]
            },
            {
              "key_code": "japanese_eisuu"
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^com.googlecode.iterm2",
                "^com.microsoft.VSCode",
                "^com.jetbrains.intellij"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "Altキー単体で英数・かなキー送信(左Altキーは英数、右Altキーはかな）",
      "manipulators": [
        {
          "from": {
            "key_code": "left_option",
            "modifiers": {
              "optional": ["any"]
            }
          },
          "parameters": {
            "basic.to_if_held_down_threshold_milliseconds": 100
          },
          "to": [
            {
              "key_code": "left_option",
              "lazy": true
            }
          ],
          "to_if_alone": [
            {
              "key_code": "japanese_eisuu"
            }
          ],
          "to_if_held_down": [
            {
              "key_code": "left_option"
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "right_option",
            "modifiers": {
              "optional": ["any"]
            }
          },
          "parameters": {
            "basic.to_if_held_down_threshold_milliseconds": 100
          },
          "to": [
            {
              "key_code": "right_option",
              "lazy": true
            }
          ],
          "to_if_alone": [
            {
              "key_code": "japanese_kana"
            }
          ],
          "to_if_held_down": [
            {
              "key_code": "right_option"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "ctrl-3 to Slack",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "3",
            "modifiers": {
              "mandatory": ["control"]
            }
          },
          "to": [
            {
              "shell_command": "open -a 'Slack'"
            }
          ]
        }
      ]
    },
